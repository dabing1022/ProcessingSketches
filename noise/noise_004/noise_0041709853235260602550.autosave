PImage img;
void setup() {
  size(640, 400);
  img = loadImage("ironman.jpeg");
  img.loadPixels();
}


void draw() {
  translate((width - img.width) / 2, (height - img.height) / 2);

  int x = int(random(img.width));
  int y = int(random(img.height));
  int index = y * img.width + x;
  int c = img.pixels[index];
  float r = red(c);
  float g = green(c);
  float b = blue(c);
  float a = alpha(c);

  stroke(r, g, b, a);
  strokeWeight(10);
  point(x, y);
}
